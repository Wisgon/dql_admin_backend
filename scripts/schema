# Define Types
type User {
  user_name
  pwd
  port
  phone_number
  email
  member_sn
  avatar
  projects
}

type Project {
  name
  description
  predicates
  types
  apis
}

# Define Directives and index
user_name: string @index(hash) @upsert .
pwd: password .
port: int @index(int) @upsert .
phone_number: string @index(hash) @upsert .
email: string @index(hash) @upsert .
member_sn: string @index(hash) @upsert .
avatar: string .
projects: [uid] .

name: string @index(hash) @upsert .
description: string .
predicates: string @index(term) .
types: string @index(term) .
apis: string @index(term) .
