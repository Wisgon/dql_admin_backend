# =============Define Types
type User {
  user_name
  pwd
  phone_number
  avatar
  create_time
  update_time
  role
}

type Role {
  role_id
  name
}


# =============Define Predicates

user_name: string @index(hash) @upsert .
pwd: string .
phone_number: string @index(hash) @upsert .
avatar: string .
role: [uid] @reverse .

name: string @index(hash) .
role_id: string @index(hash) @upsert .
create_time: datetime @index(day) .
update_time: datetime @index(day) .
