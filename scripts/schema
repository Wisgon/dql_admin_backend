# =============Define Types
type User {
  user_name
  pwd
  phone_number
  avatar
  role
}

type Role {
  name
}


# =============Define Predicates
# User
user_name: string @index(hash) @upsert .
pwd: password .
phone_number: string @index(hash) @upsert .
avatar: string .
role: [uid] @reverse .

# Role
name: string @index(hash) .

